package com.student;
import org.springframework.context.annotation.Bean;
import org.springframework.context.annotation.ComponentScan;
import org.springframework.context.annotation.Configuration;

import com.student2.*;


//@ComponentScan("com.student2")
//@ComponentScan("com.student")
//@ComponentScan("com.*")
//@ComponentScan("org.*")

@Configuration
@ComponentScan(basePackages = {"com.*","rahul.*"})
public class SpringBeanCOnfiguration {
	
	@Bean("College2 by bean method")
	public COllege2 COllege2()
	{
		System.out.println("College2 by bean method");
		return new COllege2();
	}

	
	@Bean
	public COllege2 COllege2_a()
	{
		System.out.println("College2 by bean method");
		return new COllege2();
	}


}
